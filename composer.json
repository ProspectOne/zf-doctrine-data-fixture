{ 
    "name": "prospect-one/zf-doctrine-data-fixture",
    "description": "Doctrine Data Fixture Console Route with Service Manager configured Fixtures",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "doctrine",
        "data-fixture",
        "module",
        "zf2"
    ],
    "homepage": "http://www.doctrine-project.org/",
    "authors": [
        {
            "name": "Martin Shwalbe",
            "email": "martin.shwalbe@gmail.com"
        },
        {
            "name": "Tom Anderson",
            "email": "tom.h.anderson@gmail.com"
        }
    ],
    "require": {
        "php": "~5.6 || ^7.0",
        "zendframework/zend-modulemanager": "^2.7 || ^3.0",
        "zendframework/zend-eventmanager": "^3.0",
        "zendframework/zend-servicemanager": "^3.0",
        "doctrine/data-fixtures": "1.1.*"
    },
    "require-dev": {
        "phpunit/phpunit" : "^5.4",
        "squizlabs/php_codesniffer": "*",
        "doctrine/doctrine-orm-module": "^0.10 || ^1.0",
        "zendframework/zendframework": "^3.0"   
    },
    "autoload": {
        "psr-4": {
            "ZF\\Doctrine\\DataFixture\\": "src/"
        },
        "classmap": [
          "Module.php"
        ]
    }
}
